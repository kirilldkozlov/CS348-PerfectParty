<p id="notice"><%= notice %></p>

<h2>Supplier <%= @supplier.id %></h2>

<p>
  <strong>First name:</strong>
  <%= @supplier.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @supplier.last_name %>
</p>

<p>
  <strong>Telephone:</strong>
  <%= @supplier.telephone %>
</p>

<p>
  <strong>Events supplied:</strong>
  <%= @supplier.events_supplied %>
</p>

<%= link_to 'Edit', edit_supplier_path(@supplier), class: "btn btn-primary" %>
<%= link_to 'Delete', @supplier, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
