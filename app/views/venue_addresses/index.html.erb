<p id="notice"><%= notice %></p>

<h1>Venue Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Venue</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venue_addresses.each do |venue_address| %>
      <tr>
        <td><%= venue_address.id %></td>
        <td><%= venue_address.venue %></td>
        <td><%= venue_address.address %></td>
        <td><%= link_to 'Show', venue_address %></td>
        <td><%= link_to 'Edit', edit_venue_address_path(venue_address) %></td>
        <td><%= link_to 'Destroy', venue_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue Address', new_venue_address_path %>
